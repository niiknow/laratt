{
    "name": "niiknow/laratt",
    "type": "library",
    "description": "A Laravel 5 package to support table multitenancy",
    "keywords": [
        "niiknow",
        "database",
        "laratt"
    ],
    "homepage": "https://github.com/niiknow/laratt",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Noogen",
            "email": "friends@niiknow.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "league/csv": "^9.1",
        "league/flysystem": "^1.0.27",
        "rap2hpoutre/fast-excel": "^0.11.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Niiknow\\Laratt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niiknow\\Laratt\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niiknow\\Laratt\\LarattServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
