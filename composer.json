{
    "name": "niiknow/laratt",
    "description": "A Laravel 5 package to support table multitenancy",
    "keywords": [
        "niiknow",
        "database",
        "laratt"
    ],
    "homepage": "https://github.com/niiknow/laratt",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Noogen",
            "email": "friends@niiknow.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "league/flysystem": "^1.0.27",
        "league/csv": "^9.1",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "niiknow\\laratt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpcs:fix": "phpcbf -w"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "niiknow\\laratt\\LarattServiceProvider"
            ]
        }
    }
}
