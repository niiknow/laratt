{
    "name": "niiknow/laratt",
    "type": "library",
    "description": "A Laravel 5 package to support table multitenancy",
    "keywords": [
        "niiknow",
        "database",
        "laratt"
    ],
    "homepage": "https://github.com/niiknow/laratt",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Noogen",
            "email": "friends@niiknow.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "^5.1|^6",
        "league/csv": "^9.2",
        "league/flysystem": "^1.0.50",
        "maatwebsite/excel": "^3.1",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2.2",
        "orchestra/testbench": "~3.8.1",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Niiknow\\Laratt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Niiknow\\Laratt\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niiknow\\Laratt\\LarattServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "phpcs": "phpcs",
        "phpcs:fix": "phpcbf -w"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
